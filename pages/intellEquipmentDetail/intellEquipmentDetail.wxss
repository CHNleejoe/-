.top-padding {
  height: 30rpx;
  width: 100%;
}
.base {
  min-height: 270rpx;
  box-sizing: border-box;
  background: white;
  width: 100%;
  padding: 16rpx 30rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  color: #333333;
  line-height: 60rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.base .status {
  width: 120rpx;
  height: 60rpx;
  position: absolute;
  border-radius: 30rpx 0rpx 0rpx 30rpx;
  font-size: 28rpx;
  text-align: center;
  font-family: PingFang SC;
  color: #ffffff;
  line-height: 60rpx;
  right: 0;
  top: 13rpx;
}
.base .status.active {
  background: #0FC37C;
}
.base .status.warning {
  background: #FD545C;
}
.tabs-section .van-tab__pane {
  background: white;
}
.tabs-section .van-tab--active {
  transition: all .1s;
  color: #3091F2;
}
.tabs-section .monitor .echart_panel {
  height: 500rpx;
  width: 100%;
}
.tabs-section .monitor .charts-btns {
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabs-section .monitor .charts-btns .charts-btns-item {
  width: 20%;
}
.tabs-section .monitor .charts-btns .charts-btns-item button {
  width: 100%;
}
.tabs-section .monitor .charts-btns .time {
  width: 40%;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  color: #666666;
}
.tabs-section .monitor .table {
  width: 710rpx;
  min-height: 300rpx;
  box-sizing: border-box;
  margin: 0 20rpx;
}
.tabs-section .monitor .table .flex-1 {
  flex: 1 !important;
}
.tabs-section .monitor .table .table-head {
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 1rpx solid rgba(51, 51, 51, 0.8);
  background: rgba(51, 51, 51, 0.8);
}
.tabs-section .monitor .table .table-head .table-head-item {
  flex: 2;
  color: white;
  font-size: 24rpx;
  border-top: 1rpx solid rgba(51, 51, 51, 0.8);
  border-bottom: 1rpx solid rgba(51, 51, 51, 0.8);
  border-left: 1rpx solid rgba(51, 51, 51, 0.8);
  box-sizing: border-box;
  border-right: 0;
  text-indent: 12rpx;
  font-family: PingFang SC;
}
.tabs-section .monitor .table .table-list {
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 1rpx solid rgba(51, 51, 51, 0.8);
  height: 70rpx;
}
.tabs-section .monitor .table .table-list .table-list-item {
  flex: 2;
  font-size: 24rpx;
  border-top: 1rpx solid rgba(51, 51, 51, 0.8);
  border-bottom: 1rpx solid rgba(51, 51, 51, 0.8);
  border-left: 1rpx solid rgba(51, 51, 51, 0.8);
  box-sizing: border-box;
  border-right: 0;
  text-indent: 12rpx;
  height: 100%;
  font-family: PingFang SC;
  color: #666666;
}
.tabs-section .warning .warning-item {
  background: white;
  min-height: 180rpx;
  width: 100%;
  padding-left: 30px;
}
.tabs-section .warning .warning-item:last-child .inner {
  border: 0;
}
.tabs-section .warning .warning-item .inner {
  width: 100%;
  height: 100%;
  padding: 20rpx 0rpx;
  padding-left: 45rpx;
  background: url('data:image/jpeg;base64,') no-repeat top left;
  background-size: 27rpx 34rpx;
  background-position-y: 22rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(233, 233, 233, 0.8);
}
.tabs-section .warning .warning-item .inner .p {
  font-size: 28rpx;
  font-family: PingFang SC;
  color: #666666;
}
.tabs-section .warning .warning-item .inner .p.time {
  line-height: 32rpx;
  padding-bottom: 10rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
