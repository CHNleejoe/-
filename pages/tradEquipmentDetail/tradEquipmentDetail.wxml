<view class="top-padding" hover-class="none" hover-stop-propagation="false">
</view>
<view class="base" hover-class="none" hover-stop-propagation="false">
    <view class="p" hover-class="none" hover-stop-propagation="false">
        SN码：X502PX
    </view>
    <view class="p" hover-class="none" hover-stop-propagation="false">
        SN码：X502PX
    </view>
    <view class="p" hover-class="none" hover-stop-propagation="false">
