.to-detail {
  width: 100%;
  height: 90rpx;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
  background: white;
  font-size: 32rpx;
  font-family: PingFang SC;
  text-decoration: underline;
  color: #3091f2;
  line-height: 90rpx;
  text-align: right;
}
.check-radio {
  padding: 20rpx 30rpx 60rpx;
  margin-bottom: 21rpx;
  box-sizing: border-box;
  background: white;
}
.check-radio .title {
  font-size: 28rpx;
  font-family: PingFang SC;
  color: #333333;
  margin: 9rpx 0;
}
.check-radio .radio-item {
  font-size: 28rpx;
  font-family: PingFang SC;
  color: #666666;
  line-height: 60rpx;
  margin-top: 33rpx;
}
.check-radio .radio-item .radio-group {
  display: flex;
  justify-content: space-between;
  width: 426rpx;
}
.camera {
  margin: 0 0 0 30rpx;
}
.btn {
  bottom: 43rpx;
  box-sizing: border-box;
  padding: 30rpx;
  width: 100%;
}
.btn button {
  width: 100%;
}
